{
  "name": "react-ant-design-boilerplate",
  "version": "0.1.0",
  "description": "react ant design boilerplate",
  "main": "index.js",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "start:mock": "babel-node tools/mock.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js $(find src -name '*.spec.js') --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html mocha -- --require ./tools/testSetup.js $(find src -name '*.spec.js') --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js $(find src -name '*.test.js') && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "antd": "^2.13.0",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.6",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.2",
    "jsdom": "^9.6.0",
    "lodash": "^4.16.6",
    "moment": "^2.14.1",
    "query-string": "^4.2.2",
    "react": "^15.4.0",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.0-rc.1",
    "redux": "^3.5.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-antd": "0.5.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.14.3",
    "chai": "^3.5.0",
    "chokidar": "^1.7.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.28.2",
    "enzyme": "2.4.1",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "6.2.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.0.5",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "isparta": "^4.0.0",
    "istanbul": "0.4.4",
    "json-server": "0.8.19",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "less-vars-to-js": "^1.2.0",
    "mocha": "^3.5.0",
    "npm-run-all": "3.0.0",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.5",
    "pre-commit": "1.x",
    "react-addons-perf": "^15.3.2",
    "react-addons-test-utils": "^15.6.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "^2.4.0",
    "rimraf": "2.5.4",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "wushengzhu <wushengzhu@corp.easenet.com>",
  "license": "MIT"
}
